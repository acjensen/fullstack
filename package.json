{
  "name": "fullstack",
  "version": "1.0.0",
  "bin": {
    "cdk": "cdk/cdk.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "cdk deploy --require-approval=never",
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:test": "jest",
    "cdk": "cdk",
    "test:dev": "next dev & (./wait-for-server.sh && npx cypress run --e2e --browser=chrome) & wait",
    "test:dev:open": "next dev & (./wait-for-server.sh && npx cypress open --e2e --browser=chrome) & wait",
    "test:prod": "npx cypress run --e2e --browser=chrome --config baseUrl=https://acjensen-desktop.com",
    "test:prod:open": "npx cypress open --e2e --browser=chrome --config baseUrl=https://acjensen-desktop.com"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.29",
    "@types/react": "18.2.67",
    "aws-cdk": "2.133.0",
    "cypress": "^13.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.538.0",
    "aws-cdk-lib": "2.133.0",
    "constructs": "^10.0.0",
    "next": "^14.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  }
}
