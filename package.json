{
  "name": "fullstack",
  "version": "1.0.0",
  "bin": {
    "cdk": "cdk/cdk.js"
  },
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "deploy": "cdk deploy --require-approval=never",
    "deploy:watch": "cdk watch --no-hotswap",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "synth": "cdk synth",
    "image:build": "docker build .",
    "image:run": "docker run -p 80:80 -v $HOME/.aws:/root/.aws:ro -it $(docker build -q .)",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:browser": "npm run test:browser:dev",
    "test:browser:dev": "next dev -p 3010 & (./wait-for-server-3010.sh && npx cypress run --no-runner-ui --e2e --browser=chrome --config baseUrl=http://localhost:3010) & wait",
    "test:browser:dev:open": "next dev -p 3000 & (./wait-for-server-3000.sh && npx cypress open --e2e --browser=chrome --config baseUrl=http://localhost:3000) & wait",
    "test:browser:prod": "npx cypress run --no-runner-ui --e2e --browser=chrome --config baseUrl=https://acjensen-desktop.com",
    "test:browser:prod:open": "npx cypress open --e2e --browser=chrome --config baseUrl=https://acjensen-desktop.com",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf cdk.out && rm -rf coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.29",
    "@types/react": "18.2.67",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "aws-cdk": "^2.133.0",
    "cypress": "^13.7.0",
    "daisyui": "^4.7.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.538.0",
    "@aws-sdk/util-dynamodb": "^3.540.0",
    "aws-cdk-lib": "2.133.0",
    "bcrypt-ts": "^5.0.2",
    "constructs": "^10.0.0",
    "next": "^14.1.4",
    "next-auth": "^5.0.0-beta.15",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  }
}
